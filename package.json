  "keywords": [
    "box2d",
    "physics",
    "snowboard",
    "endless runner"
  ],
  "author": "Andreas Schoch",
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^10.2.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "dependencies": {
    "phaser": "^3.55.2",
    "planck-js": "^0.3.31",
    "simplex-noise": "^3.0.1",
    "simplify-js": "^1.2.4"
  }
